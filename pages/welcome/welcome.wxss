page {
  height: 100%;
  background-color: #b3d4db;
}

.container-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: #b3d4db;
}

.img-box {
  width: 200rpx;
  height: 200rpx;
  margin-top: 160rpx;
}

.text-box {
  margin-top: 100rpx;
  font-size: 32rpx;
  font-weight: bold;
}

.btn-box {
  margin-top: 200rpx;
  border: 1px solid #405f80;
  width: 200rpx;
  height: 80rpx;
  text-align: center;
  border-radius: 5px;
  line-height: 80rpx;
}
.btn{
  font-size:22rpx;
  font-weight:bold;
  color:#405f80;
}